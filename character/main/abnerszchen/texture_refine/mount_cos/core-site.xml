<configuration>
  <property>
    <name>fs.cosn.impl</name>
    <value>org.apache.hadoop.fs.CosFileSystem</value>
  </property>
  <property>
    <name>fs.cosn.useHttps</name>
    <value>false</value>
  </property>
  <property>
    <name>fs.cosn.userinfo.secretId</name>
    <value>secretId</value>
  </property>
  <property>
    <name>fs.cosn.userinfo.secretKey</name>
    <value>secretKey</value>
  </property>
  <property>
    <name>fs.cosn.read.ahead.queue.size</name>
    <value>24</value>
  </property>
  <property>
    <name>fs.cosn.read.ahead.block.size</name>
    <value>4194304</value>
  </property>
  <property>
    <name>fs.cosn.upload_thread_pool</name>
    <value>48</value>
  </property>
  <property>
    <name>fs.cosn.tmp.dir</name>
    <value>/data/goosefs-lite-tmp/</value>
  </property>
  <property>
    <name>fs.cosn.posix_extension.enabled</name>
    <value>false</value>
  </property>
  <property>
    <name>fs.cosn.posix_extension.tmp.dir.quota</name>
    <value>10737418240</value>
  </property>
  <property>
    <name>fs.cosn.filestatus.list.recursive.enabled</name>
    <value>true</value>
  </property>
  <property>
    <name>fs.cosn.bucket.endpoint_suffix</name>
    <value>cos-internal.ap-chongqing.tencentcos.cn</value> 
  </property>

</configuration>